<!--====================================================
  CONTACT SECTION
======================================================-->
<section id="contact" class="contact-section">
    <div class="container contact-container">
        <div class="row">
            <div class="heading wow fadeIn" align="center">
                <h2 class="contact-title">Contact Me</h2>
            </div>
        </div>
        <div class="row contact-row">
            <div class="col-lg-3 col-md-2 col-sm-0"></div>
            <div class="col-lg-3 col-md-5 col-sm-7 message">
                <h4 class="h4-contact">SEND ME A MESSAGE</h4>
                <hr>
                <form action="/send" method="POST">
                    <div class="form-group">

                        <input type="name" class="form-control" id="name" placeholder="Your Name" name="name">
                    </div>
                    <div class="form-group">

                        <input type="email" class="form-control" id="email" placeholder="Email Address" name="email">
                    </div>
                    <div class="form-group">

                        <textarea type="message" class="form-control" rows="7" id="comment" placeholder="Your Message" name="message"></textarea>
                    </div>
                    <div>{{msg}}</div>
                    <button type="submit" class="btn btn-message">Send</button>
                </form>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-5 other-channels">
                <h4 class="h4-contact">OTHER CHANNELS</h4>
                <hr>
                <p>
                    <i class="fa fa-map-marker" aria-hidden="true" style="font-size:10px"></i>
                    <strong>Address:</strong>
                    <br>1234 Company Address
                    <br> City, State
                    <br> United States
                    <br>
                    <i class="fa fa-phone" aria-hidden="true" style="font-size:10px"></i>
                    <strong>Phone:</strong> +1 555 555-5555
                    <br>

                    <i class="fa fa-envelope" aria-hidden="true" style="font-size:10px"></i>
                    <strong>Email:</strong>
                    <a href="#" style="color:#fff"> email@email.com
                        <br>
                    </a>
                </p>

                <div class="social-links">
                    <a href="https://twitter.com/" class="twitter">
                        <i class="fab fa-twitter fa-md"></i>
                    </a>
                    <a href="https://www.instagram.com/" class="instagram">
                        <i class="fab fa-instagram fa-md"></i>
                    </a>
                    <a href="https://plus.google.com/" class="google-plus">
                        <i class="fab fa-google-plus fa-md"></i>
                    </a>
                    <a href="https://www.linkedin.com/" class="linkedin">
                        <i class="fab fa-linkedin-in fa-md"></i>
                    </a>
                </div>
            </div>
            <div class="col-lg-3 col-md-2 col-sm-0"></div>
        </div>
    </div>
</section>
